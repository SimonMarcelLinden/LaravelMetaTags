{
    "name": "simonmarcellinden/laravelmetatags",
    "description": "With this package you can manage header Meta Tags from Laravel controllers.",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Marcel Linden",
            "email": "info@snerve.de"
        }
    ],
    "homepage": "https://github.com/simonmarcellinden/laravelmetatags",
    "keywords": ["Laravel", "LaravelMetaTags", "MetaTag", "MetaTags"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "SimonMarcelLinden\\LaravelMetaTags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimonMarcelLinden\\LaravelMetaTags\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimonMarcelLinden\\LaravelMetaTags\\MetaTagsServiceProvider"
            ],
            "aliases": {
                "MetaTag": "SimonMarcelLinden\\LaravelMetaTags\\Facades\\MetaTag"
            }
        }
    }
}
